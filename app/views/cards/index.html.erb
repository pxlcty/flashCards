<p id="notice"><%= notice %></p>

<h1>Listing Cards</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cards.each do |card| %>
      <tr>
        <td><%= link_to card.front, card %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<h5><%= link_to 'New Card', new_card_path %></h5>
